<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"   
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.apps.omnipotent.manager.dao.ImgDao">
     <select id="findByCodeAndId" resultType="com.apps.omnipotent.manager.bean.Img">
        select t.* from be_img t where t.code = #{code} and t.unique_key=#{id}
    </select>
</mapper>