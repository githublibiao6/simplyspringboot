<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"   
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.apps.omnipotent.manager.dao.ProjectDao">
    <!-- 根据条件获得project list -->
    <select id="listAdmin" resultType="com.apps.omnipotent.manager.bean.Admin">
        select *
        from project m
    </select>
    <!-- 根据id获得project详情 -->
    <select id="getAdmin" resultType="com.apps.omnipotent.manager.bean.Admin">
        select *
        from project m where m.id = #{id}
    </select>
</mapper>